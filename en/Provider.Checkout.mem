Checkout(AssetList,CheckoutMode)
Checkout(string[],CheckoutMode)
Checkout(Object[],CheckoutMode)
Checkout(Asset,CheckoutMode)
Checkout(string,CheckoutMode)
Checkout(Object,CheckoutMode)
